class Solution {
    public List<Boolean> kidsWithCandies(int[] candies, int extraCandies) {
        ArrayList<Boolean> res = new ArrayList<>();
        int max = Integer.MIN_VALUE;
        for(int i=0;i<candies.length;i++){
            if(candies[i]>max){
                max = candies[i];
            }
        }
        
        for(int i=0;i<candies.length;i++){
            if(candies[i]+extraCandies >=max){
                res.add(true);
            }else{
                res.add(false);
            }
        }
        return res;
    }
}
